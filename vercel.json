{
  "version": 2,
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/**": {
      "runtime": "python3.11",
      "excludeFiles": "{etl/**,tests/**,frontend/**,.venv/**,node_modules/**,.git/**,.vercel/**}",
      "includeFiles": "api/wheels/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/$1"
    }
  ]
}