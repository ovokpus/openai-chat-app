{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "frontend/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "documents/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/documents/(.*)",
      "dest": "documents/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "frontend/dist/assets/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "frontend/dist/fonts/$1"
    },
    {
      "src": "/vite.svg",
      "dest": "frontend/dist/vite.svg"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist /index.html"
    }
  ]
}